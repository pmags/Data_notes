    "# For a dictionary to work correctly as intended, keys should be unique\n",
    "# We can add information to a dictionary the following way\n",
    "world[\"sealand\"] = 0.000027\n",
    "print(world)\n",
    "\n",
    "\"sealand\" in world\n",
    "\n",
    "# in order to delete we use del()\n",
    "del(world[\"sealand\"])\n",
    "print(world)"
   ]
  },
