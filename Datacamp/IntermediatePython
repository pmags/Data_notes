    "\n",
    "\n",
    "# Print out the capital of France\n",
    "europe[\"france\"][\"capital\"]\n",
    "\n",
    "# Create sub-dictionary data\n",
    "data = {\"capital\":\"rome\", \"population\":59.83}\n",
    "\n",
    "# Add data to europe under key 'italy'\n",
    "europe[\"italy\"] = data\n",
    "\n",
    "# Print europe\n",
    "print(europe)"
   ]
